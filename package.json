{
  "name": "@capaj/dotenv-to-x",
  "version": "0.0.2",
  "description": "Convert environment variables between different cloud providers",
  "main": "./dist/index.js",
  "bin": {
    "dotenv-to-gh": "./dist/github-cli.mjs",
    "dotenv-to-cf": "./dist/cloudflare-cli.mjs"
  },
  "scripts": {
    "test": "vitest",
    "ts": "tsc --noEmit",
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky install"
  },
  "keywords": [
    "github",
    "cloudflare",
    "secrets"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "husky": "^9.1.7",
    "prettier": "^3.4.1",
    "pretty-quick": "^4.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@inquirer/prompts": "^7.1.0"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write",
    "*": "prettier --ignore-unknown --write"
  },
  "tsup": {
    "external": [
      "bun"
    ],
    "dts": true,
    "format": [
      "esm"
    ],
    "entry": [
      "src/github-cli.ts",
      "src/cloudflare-cli.ts"
    ]
  }
}
